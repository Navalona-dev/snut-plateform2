{% extends 'base_admin.html.twig' %}

{% block title %}District{% endblock %}

{% block body %}
    <h1>District</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ district.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ district.Nom }}</td>
            </tr>
            <tr>
                <th>Superficie</th>
                <td>{{ district.Superficie }}</td>
            </tr>
            <tr>
                <th>Population</th>
                <td>{{ district.Population }}</td>
            </tr>
            <tr>
                <th>Densite</th>
                <td>{{ district.Densite }}</td>
            </tr>
            <tr>
                <th>IsEligibleForCreni</th>
                <td>{{ district.isEligibleForCreni ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>IsEligibleForCrenas</th>
                <td>{{ district.isEligibleForCrenas ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ district.type }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_district_index') }}">back to list</a>

    <a href="{{ path('app_district_edit', {'id': district.id}) }}">edit</a>

    {{ include('admin2/district/_delete_form.html.twig') }}
{% endblock %}
