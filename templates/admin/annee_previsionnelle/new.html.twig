{% extends 'base_admin.html.twig' %}

{% block title %}Année{% endblock %}
{% block stylesheets %}
	<link rel="stylesheet" href="{{ asset('assets/css/select2.min.css') }}" type="text/css"/>
{% endblock %}
{% block body %}
<div class="col-md-12">
		<div class="card ">
			<div class="card-header card-header-green card-header-text">
				<div class="card-text">
					<h4 class="card-title">Ajouter année</h4>
				</div>
			</div>
			<div class="card-body ">
			{{ form_start(form, {'action': path('app_centre_hospitalier_universitaire_new'), 'method': 'POST', 'attr': {'enctype':'multipart/form-data', 'class': 'form-horizontal'}}) }}
                <div class="row">
				
                <label class="col-sm-2 col-form-label">Année</label>
					<div class="col-sm-10">
						<div class="form_group">
							{{ form_widget(form.Annee) }}
						</div>
					</div>
				</div>
                  <div class="row">
                <label class="col-sm-2 col-form-label">Valeur de calcul pour ATPE 1er collecte</label>
					<div class="col-sm-10">
						<div class="form_group">
							{{ form_widget(form.ValeurCalculTheoriqueATPE01) }}
						</div>
					</div>
				</div>
              
				<div class="row">
					<label class="col-sm-2 col-form-label">Valeur de calcul pour AMOX 1er collecte</label>
					<div class="col-sm-10">
						<div class="form_group">
							{{ form_widget(form.ValeurCalculTheoriqueAMOX01) }}
						</div>
					</div>
				</div>
				<div class="row  mt-3">
					<label class="col-sm-2 col-form-label">Valeur de calcul pour Fiche Patient 1er collecte</label>
					<div class="col-sm-10">
						<div class="form-group">
							{{ form_widget(form.ValeurCalculTheoriqueFichePatient01) }}
						</div>
					</div>
				</div>
				<div class="row  mt-3">
					<label class="col-sm-2 col-form-label">Valeur de calcul pour le registre 1er collecte</label>
					<div class="col-sm-10">
						<div class="form-group">
							{{ form_widget(form.ValeurCalculTheoriqueRegistre01) }}
						</div>
					</div>
				</div>
                <div class="row  mt-3">
					<label class="col-sm-2 col-form-label">Valeur de calcul pour le carnet de rapport mensuel 1er collecte</label>
					<div class="col-sm-10">
						<div class="form-group">
							{{ form_widget(form.ValeurCalculTheoriqueCarnetRapport01) }}
						</div>
					</div>
				</div>
                <div class="row  mt-3">
					<label class="col-sm-2 col-form-label">Valeur de calcul pour ATPE autres collectes</label>
					<div class="col-sm-10">
						<div class="form-group">
							{{ form_widget(form.ValeurCalculTheoriqueATPE02) }}
						</div>
					</div>
				</div>
                <div class="row">
					<label class="col-sm-2 col-form-label">Valeur de calcul pour AMOX autres collectes</label>
					<div class="col-sm-10">
						<div class="form_group">
							{{ form_widget(form.ValeurCalculTheoriqueAMOX02) }}
						</div>
					</div>
				</div>
                 <div class="row">
					<label class="col-sm-2 col-form-label">Valeur de calcul pour Fiche Patient autres collectes</label>
					<div class="col-sm-10">
						<div class="form_group">
							{{ form_widget(form.ValeurCalculTheoriqueFichePatient02) }}
						</div>
					</div>
				</div>
				 <div class="row">
					<label class="col-sm-2 col-form-label">Valeur de calcul pour le registre autres collecte</label>
					<div class="col-sm-10">
						<div class="form_group">
							{{ form_widget(form.ValeurCalculTheoriqueRegistre02) }}
						</div>
					</div>
				</div>
				 <div class="row">
					<label class="col-sm-2 col-form-label">Valeur de calcul pour le carnet de rapport mensuel autres collecte</label>
					<div class="col-sm-10">
						<div class="form_group">
							{{ form_widget(form.ValeurCalculTheoriqueCarnetRapport02) }}
						</div>
					</div>
				</div>
			</div>
			<div class="card-footer ">
				{{ form_widget(form.save) }}
			</div>
			{{ form_end(form) }}
		</div>
	</div>
{% endblock %}
{% block javascripts %}
	<script src="{{asset('assets/js/select2.min.js')}}"></script>
	<script>
		$(document).ready(function () {
	
				$(".mul-select").select2({
				placeholder: "Choisir province", // placeholder
				tags: true,
				tokenSeparators: ['/', ',', ';', " "]
				});

		});

		
	</script>
{% endblock %}
